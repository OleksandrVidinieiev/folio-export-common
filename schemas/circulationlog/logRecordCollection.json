{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Collection of circulation audit log records",
  "LogRecordCollection": {
    "type": "object",
    "properties": {
      "logRecords": {
        "type": "array",
        "description": "Circulation audit log records",
        "items": {
          "$ref": "logRecord.json#/LogRecord"
        }
      },
      "totalRecords": {
        "type": "integer"
      }
    },
    "required": [
      "logRecords",
      "totalRecords"
    ]
  }
}
