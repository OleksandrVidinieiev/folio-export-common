{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Provider data object schema",
  "ProviderData": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier of provider",
        "example": "19"
      },
      "type": {
        "type": "string",
        "description": "Type of resource",
        "example": "providers"
      },
      "attributes": {
        "type": "object",
        "description": "Provider object data attributes",
        "$ref": "providerAttributes.json#/ProviderAttributes"
      }
    },
    "required": [
      "id",
      "attributes"
    ]
  }
}
